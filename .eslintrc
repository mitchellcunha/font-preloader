{
  "globals": {
    "describe": true,
    "context": true,
    "xdescribe": true,
    "expect": true,
    "it": true,
    "xit": true,
    "before": true,
    "after": true,
    "beforeEach": true,
    "afterEach": true,
    "require": true,
    "exports": true,
    "module": true,
    "process": true,
    "global": true
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "rules": {
    "semi": [
      2,
      "always"
    ],
    "dot-notation": 0,
    "no-bitwise": 2,
    "curly": 2,
    "eqeqeq": 2,
    "guard-for-in": 1,
    "wrap-iife": [
      2,
      "any"
    ],
    "no-use-before-define": 2,
    "new-cap": 0,
    "no-caller": 2,
    "no-new": 2,
    "no-plusplus": 2,
    "no-empty": 2,
    "no-undef": 2,
    "no-unused-vars": [
      2,
      {
        "varsIgnorePattern": "React"
      }
    ],
    "no-eq-null": 2,
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "strict": 0,
    "no-eval": 2,
    "quotes": [
      2,
      "single"
    ],
    "max-len": [
      2,
      80
    ],
    "max-params": [
      2,
      5
    ],
    "max-depth": [
      2,
      2
    ],
    "no-multiple-empty-lines": 0,
    "one-var": 0,
    "eol-last": 0,
    "space-return-throw-case": 0,
    "vars-on-top": 2
  },
  "extends": "eslint:recommended"
}